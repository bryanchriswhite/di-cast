*
  box-sizing: border-box;

html
  min-width 430px

[class*='ui-select-']
  &,
  &:hover,
  &:active,
  &:focus
    outline-style none
    outline-width 0px

audio
  position absolute
  z-index 1
  bottom 0
  right 0
  left 0
  margin auto


#hero
  width 100%
  height 356px
  position relative
  margin-top 30px

#channel-art
  border 3px solid dimgrey
  position absolute
  left 0
  right 0
  margin auto
